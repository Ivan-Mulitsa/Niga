#include<iostream>
using namespace std;

int main() {

	double a=0, b=0, c=0, min, max;
	cout << "Enter a,b,c  ";
	cin >> a>>b>>c;
	
	if (a > b)
	{
		min = b;
		max = a;
	}
	else
	{
		min = a;
		max = b;
	}

	if (c < min)
	{
		cout << c << " " << min << " " << max;
	}
	else
	{
		if (c < max)
		{
			cout << min << " " << c << " " << max;
		}
		else
		{
			cout << min << " " << max << " " << c;
		}
	}
	return 0;
}
