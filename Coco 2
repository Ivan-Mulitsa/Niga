#include<iostream>
using namespace std;

int main() {

	unsigned int N=10, K, I=0, S1=0, S2=0;
	
	do {

		cout << "Enter K=  ";
		cin >> K;
		I++;

		if (K%2)
		{
			S1 += K;
		}
		else
		{
			S2 += K;
		}

	} while (I < N);

	cout << "S1= " << S1 << endl;
	cout << "S2= " << S2 << endl;

	return 0;
}
